		{
			"name": "登录Get方式",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"状态码为200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"响应状态码在 [200,201, 202] 中\", function () {\r",
							"    pm.expect(pm.response.code).to.be.oneOf([200,201, 202]);\r",
							"});\r",
							"\r",
							"pm.test(\"是否包含字符串\\\"err_code\\\":0\", function () {\r",
							"    pm.expect(pm.response.text()).to.include('\"err_code\":0');\r",
							"});\r",
							"\r",
							"\r",
							"\r",
							"pm.test(\"判断是否有uuid字段\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.data).to.have.property(\"uuid\")\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"判断是否有token字段\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.data).to.have.property(\"token\")\r",
							"});\r",
							"\r",
							"pm.test(\"判断是否有role字段\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.data).to.have.property(\"role\")\r",
							"});\r",
							"\r",
							"\r",
							"\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
