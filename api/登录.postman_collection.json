		{
			"name": "登录Post方式",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var jsondata = pm.response.json();\r",
							"pm.globals.set(\"uuid\", jsondata.data.uuid);\r",
							"pm.globals.set(\"token\", jsondata.data.token);\r",
							"\r",
							"pm.test(\"状态码为200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"响应状态码在 [200,201, 202] 中\", function () {\r",
							"    pm.expect(pm.response.code).to.be.oneOf([200, 201, 202]);\r",
							"});\r",
							"\r",
							"pm.test(\"是否包含字符串\\\"err_code\\\":0\", function () {\r",
							"    pm.expect(pm.response.text()).to.include('\"err_code\":0');\r",
							"});\r",
							"\r",
							"pm.test(\"json值判断\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.data.err_code).to.eql(0);\r",
							"});\r",
							"\r",
							"pm.test(\"响应时间是否小于200ms\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(200);\r",
							"});\r",
							"\r",
							"pm.test(\"是否包含响应头：Content-Type\", function () {\r",
							"    pm.response.to.have.header(\"Content-Type\");\r",
							"});\r",
							"\r",
							"\r",
							"\r",
							"pm.test(\"验证相应体中指定的字段存在\", function () {\r",
							"    pm.expect(pm.response.json().data).to.have.include.keys([\"err_msg\", \"err_code\"])\r",
							"})\r",
							"\r",
							"pm.test(\"判断是否有uuid字段\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.data).to.have.property(\"uuid\")\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"判断是否有token字段\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.data).to.have.property(\"token\")\r",
							"});\r",
							"\r",
							"pm.test(\"判断是否有role字段\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.data).to.have.property(\"role\")\r",
							"});\r",
							"\r",
							"\r",
							"console.log(pm.variables.get(\"aa\"));\r",
							"\r",
							"\r",
							"\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
